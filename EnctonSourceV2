-- StarterGui.EnctonNewUI / Tryna fix size.Main.SearcherTab.SearchBarHolder.SearchBar.LocalScript
local function C_5f()
local script = G2L["5f"];
	script.Parent:GetPropertyChangedSignal("Text"):Connect(function()
		if script.Parent.Text ~= "" then
			game:GetService("TweenService"):Create(script.Parent.Parent.Header3, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0.962, 0,0.033, 0)}):Play()
		elseif script.Parent.Text == "" then
			game:GetService("TweenService"):Create(script.Parent.Parent.Header3, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0,0,0,0)}):Play()
		end
	end)
	
	
	
		local function AddTab(imageTab, scriptname, source, isUniversal)
		local scriptFrame = script.Parent.Parent.Parent.scroll
			local newList = scriptFrame.searchresultTMP.SearchResult:Clone()
			
			local execute = newList.Run
			local copy = newList.Copy
			local loadIE = newList.LIE
	
			local scname = newList.searchname
			local img = newList.searchresultimg
			local universal = newList.isUniversalTEXT
	
			newList.Name = scriptname
			newList.Parent = scriptFrame
			newList.Visible = true
	
			img.Image = imageTab
			universal.Text = isUniversal
			scname.Text = scriptname
	
			execute.MouseButton1Click:Connect(function()
				loadstring(source)()
			end)
	
			copy.MouseButton1Click:Connect(function()
				setclipboard(source)
			end)
	
			loadIE.MouseButton1Click:Connect(function()
				script.Parent.Parent.Parent.Parent.EditorTab.ScriptBoxHolder.scroll.Input.Text = source
			end)
		end
	
	
		local http = game:GetService("HttpService")
		script.Parent.FocusLost:Connect(function()
			
		game:GetService("TweenService"):Create(script.Parent.Parent.Header3, TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0,0,0,0)}):Play()
			
		for _, child in ipairs(script.Parent.Parent.Parent.scroll:GetChildren()) do
				if child:IsA("Frame") then
					child:Destroy()
				end
			end
	
	
	
	
		-- UNI: AddTab("rbxassetid://11886780412", script.title, script.script, "Is Patched: False")
		-- UN UNI: AddTab("https://assetgame.roblox.com/Game/Tools/ThumbnailAsset.ashx?aid="..script.game.gameId.."&fmt=png&wd=420&ht=420", script.title, script.script, "Is Patched: True")
	
			local url = "https://scriptblox.com/api/script/search?filters=free&q="..script.Parent.Parent.SearchBar.Text
			local response = game:HttpGetAsync(url)
			local decoded = http:JSONDecode(response)
			for _, script in pairs(decoded.result.scripts) do
				if(script.isUniversal == true) then
				AddTab("rbxassetid://11886780412", script.title, script.script, "Is Universal: True")
				else
				AddTab("https://assetgame.roblox.com/Game/Tools/ThumbnailAsset.ashx?aid="..script.game.gameId.."&fmt=png&wd=420&ht=420", script.title, script.script, "Is Patched: False")
			end
			end -- for _, script end
	end)
end;
task.spawn(C_5f);
-- StarterGui.EnctonNewUI / Tryna fix size.Main.SettingsTab.Setting1.OnOff.click.LocalScript
local function C_77()
local script = G2L["77"];
	local toggle = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if toggle == false then
		toggle = true
			
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Active = true
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Selectable = true
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Draggable = true
			
			game:GetService("TweenService"):Create(script.Parent.Parent.Circle, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(0.767, 0,0.5, 0)}):Play()
			game:GetService("TweenService"):Create(script.Parent.Parent.Circle, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(38, 120, 0)}):Play()
			game:GetService("TweenService"):Create(script.Parent.Parent.Parent.OnOff, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(47, 147, 0)}):Play()
		else
			toggle = false
			game:GetService("TweenService"):Create(script.Parent.Parent.Circle, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(0.233, 0,0.5, 0)}):Play()
			game:GetService("TweenService"):Create(script.Parent.Parent.Circle, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(91, 0, 0)}):Play()
			game:GetService("TweenService"):Create(script.Parent.Parent.Parent.OnOff, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {BackgroundColor3 = Color3.fromRGB(138, 0, 0)}):Play()
			
			
			game:GetService("TweenService"):Create(script.Parent.Parent.Parent.Parent.Parent.Parent.Main, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Position = UDim2.new(0.5, 0,0.498, 0)}):Play()
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Active = false
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Selectable = false
			script.Parent.Parent.Parent.Parent.Parent.Parent.Main.Draggable = false
			
		end
		end)
end;
task.spawn(C_77);
