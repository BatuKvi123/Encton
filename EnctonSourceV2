-- StarterGui.EnctonNewUI / Tryna fix size.Main.EditorTab.SaveScriptMenu.Save.SaveBTN.click.LocalScript
local function C_4e()
local script = G2L["4e"];
	local folderName = "EnctonSavedScripts"
	local folderPath = folderName .. "/"
	local btn = script.Parent
	
	if not isfolder(folderName) then
		makefolder(folderName)
	end
	
	local function AddScript(name, source)
		writefile(folderPath .. name, source)
	
	
		local parent = script.Parent.Parent.Parent.Parent.Parent.Parent.SavedScriptsTab.scroll
		local hub = parent.savedsctemp.SavedSC
		local newhub = hub:Clone()
	
	
		local scriptName = newhub.scname
		local execButton = newhub.Run
		local delButton = newhub.Delete
	
		newhub.Parent = parent
		newhub.Visible = true
		scriptName.Text = name
	
	
		execButton.MouseButton1Click:Connect(function()
			loadstring(readfile(folderPath .. name))()
		end)
	
	
		delButton.MouseButton1Click:Connect(function()
			newhub:Destroy()
			delfile(folderPath .. name)
		end)
	end
	
	
	for _, v in pairs(script.Parent.Parent.Parent.Parent.Parent.Parent.SavedScriptsTab.scroll:GetChildren()) do
		if v:IsA("Frame") then
			v:Destroy()
		end
	end
	wait(1)
	
	
	for _, file in ipairs(listfiles(folderPath)) do
		local fileName = file:match("([^/]+)$")
		AddScript(fileName, readfile(file))
	end
	
	
	btn.MouseButton1Click:Connect(function()
	
	
		local scriptName = script.Parent.Parent.Parent.Parent.Parent.SaveScriptMenu.Save.SCNAME.Text
		local scriptSource = script.Parent.Parent.Parent.Parent.Parent.Parent.EditorTab.ScriptBoxHolder.scroll.Input.Text
	
		if scriptName and scriptName ~= "" and scriptSource and scriptSource ~= "" then
			AddScript(scriptName, scriptSource)
			script.Parent.Parent.Parent.Parent.Parent.SaveScriptMenu.Save.SCNAME.Text = ""
			
			
			game:GetService("TweenService"):Create(script.Parent.Parent.Parent.Parent.Save, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Position = UDim2.new(0.165, 0,1, 0)}):Play()
			game:GetService("TweenService"):Create(script.Parent.Parent.Parent.Parent.Parent.SaveScriptMenu, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0), {Transparency = 1}):Play()
			wait(0.3)
			script.Parent.Parent.Parent.Parent.Parent.SaveScriptMenu.Visible = false
		end
	end)
end;
task.spawn(C_4e);
