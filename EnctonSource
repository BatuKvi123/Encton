	end
end;
task.spawn(C_26);
-- StarterGui.Encton.Main.CloudFrame.SearchBtn.LocalScript
local function C_4d()
local script = G2L["4d"];
	local loadedscripts = script.Parent.Parent.loadedscripts
	local btn = script.Parent
	
	local function AddTab(imageTab, scriptname, source, ispatched)
		local scriptFrame = script.Parent.Parent.scrollcloud
		local newList = scriptFrame.Template.FoundedScript:Clone()
	
		local execute = newList.Execute
		local copy = newList.Copy
		local loadIE = newList.loadintoeditor
	
		local scname = newList.scname
		local img = newList.scimage
		local patched = newList.ispatched
	
		newList.Name = scriptname
		newList.Parent = scriptFrame
		newList.Visible = true
	
		img.Image = imageTab
		patched.Text = ispatched
		scname.Text = scriptname
	
		execute.MouseButton1Click:Connect(function()
			loadstring(source)()
		end)
		
		copy.MouseButton1Click:Connect(function()
			setclipboard(source)
		end)
		
		loadIE.MouseButton1Click:Connect(function()
			script.Parent.Parent.Parent.ExecutorFrame.TextHolder.Input.Text = source
		end)
	end
	
	
	local http = game:GetService("HttpService")
	btn.MouseButton1Click:Connect(function()
		for _, child in ipairs(script.Parent.Parent.scrollcloud:GetChildren()) do
			if child:IsA("Frame") then
				child:Destroy()
			end
		end
	
	
		local url = "https://scriptblox.com/api/script/search?filters=free&q="..script.Parent.Parent.Input.Text
		local response = game:HttpGetAsync(url)
		local decoded = http:JSONDecode(response)
		for _, script in pairs(decoded.result.scripts) do
			if(script.isUniversal == true) then
				if(script.isPatched == true) then
					AddTab("rbxassetid://11886780412", script.title, script.script, "Is Patched: True")
				else
					AddTab("rbxassetid://11886780412", script.title, script.script, "Is Patched: False")
					end -- is patched end
				else
				if(script.isPatched == true) then
					AddTab("https://assetgame.roblox.com/Game/Tools/ThumbnailAsset.ashx?aid="..script.game.gameId.."&fmt=png&wd=420&ht=420", script.title, script.script, "Is Patched: True")
				else
					AddTab("https://assetgame.roblox.com/Game/Tools/ThumbnailAsset.ashx?aid="..script.game.gameId.."&fmt=png&wd=420&ht=420", script.title, script.script, "Is Patched: False")
					end -- is patched end
				end -- is universal end
	
		end -- for _, script end
		loadedscripts.Text = "Loaded Scripts: "..decoded.result.totalPages
	end)
	
	
	
end;
task.spawn(C_4d);
-- StarterGui.Encton.Main.ExecutorFrame.TextHolder.Input.LocalScript
local function C_68()
local script = G2L["68"];
	
	local textBox = script.Parent
	local scrollingFrame = textBox.Parent
	
	
	local initialTextBoxSize = UDim2.new(0, 321, 0, 235)
	local initialCanvasSize = UDim2.new(0, 400, 0, 300)
	local padding = 10
	
	
	textBox.Size = initialTextBoxSize
	scrollingFrame.CanvasSize = initialCanvasSize
	
	
	local function resizetxtboxyey()
		local textSize = game:GetService("TextService"):GetTextSize(
			textBox.Text,
			textBox.TextSize,
			textBox.Font,
			Vector2.new(textBox.AbsoluteSize.X, math.huge)
		)
		
	
	
		
		local newWidth = math.max(textSize.X + padding, initialTextBoxSize.X.Offset)
		local newHeight = math.max(textSize.Y + padding, initialTextBoxSize.Y.Offset)
		
	end
	
	
	local function resizeCanvas()
		local maxWidth = initialCanvasSize.X.Offset
		local maxHeight = initialCanvasSize.Y.Offset
	
	
		for _, child in ipairs(scrollingFrame:GetChildren()) do
			if child:IsA("GuiObject") then
				local childSize = child.AbsoluteSize
				local childPos = child.AbsolutePosition - scrollingFrame.AbsolutePosition
	
				maxWidth = math.max(maxWidth, childPos.X + childSize.X)
				maxHeight = math.max(maxHeight, childPos.Y + childSize.Y)
			end
		end
	
	
		scrollingFrame.CanvasSize = UDim2.new(0, maxWidth + padding, 0, maxHeight + padding)
	end
	
	
	local function updateSizes()
		resizetxtboxyey()
		resizeCanvas()
	end
	
	
	textBox:GetPropertyChangedSignal("Text"):Connect(updateSizes)
	scrollingFrame.ChildAdded:Connect(resizeCanvas)
	scrollingFrame.ChildRemoved:Connect(resizeCanvas)
	
	
	updateSizes()
	
end;
task.spawn(C_68);
