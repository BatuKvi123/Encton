-- StarterGui.Encton.Main.ExecutorFrame.Server-ClientButtons
local function C_83()
local script = G2L["83"];
	-- closed : Color3.new(49, 49, 49)
	-- selected : Color3.new(24, 24, 24)
	
	local cs = script.Parent["Client-Side"]
	local executiontype = script.Parent.Parent.Parent.ExecutionType
	
	
	cs.MouseButton1Click:Connect(function()
		game:GetService("TweenService"):Create(cs, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {BackgroundColor3 = Color3.new(24, 24, 24)}):Play()
		executiontype.Value = 0
	end)
end;
task.spawn(C_83);
-- StarterGui.Encton.Main.ExecutorFrame.SaveEditor
local function C_84()
local script = G2L["84"];
	-- file name
	local fileName = "EnctonWorkspace/EnctonEditorText.lua"
	
	
	-- loading the file yey
	if isfile(fileName) then
		local savedText = readfile(fileName)
		script.Parent.TextHolder.Input.Text = savedText
	end
	
	-- savin' the file
	script.Parent.TextHolder.Input:GetPropertyChangedSignal("Text"):Connect(function()
		local currentText = script.Parent.TextHolder.Input.Text
		writefile(fileName, currentText)
	end)
	
end;
task.spawn(C_84);
-- StarterGui.Encton.Main.ButtonHandler
local function C_85()
local script = G2L["85"];
	-- btn tween
	local function tween(object, mode)
		if mode == "open" then
			game:GetService("TweenService"):Create(object, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {BackgroundTransparency = 0.9}):Play()
		elseif mode == "close" then
			game:GetService("TweenService"):Create(object, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.In, 0, false, 0), {BackgroundTransparency = 1}):Play()
		end
	end
	
	--locals
	local hbtn = script.Parent.Home
	local ebtn = script.Parent.Executor
	local sbtn = script.Parent.Settings
	local cbtn = script.Parent.Cloud
	
	local hpage = script.Parent.HomeFrame
	local epage = script.Parent.ExecutorFrame
	local cpage = script.Parent.CloudFrame
	local spage = script.Parent.SettingsFrame
	
	--codes
	hbtn.MouseButton1Click:Connect(function()
		tween(hbtn, "open")
		tween(ebtn, "close")
		tween(sbtn, "close")
		tween(cbtn, "close")
		
		hpage.Visible = true
		epage.Visible = false
		cpage.Visible = false
		spage.Visible = false
	end)
	
	ebtn.MouseButton1Click:Connect(function()
		tween(ebtn, "open")
		tween(hbtn, "close")
		tween(sbtn, "close")
		tween(cbtn, "close")
		
		hpage.Visible = false
		epage.Visible = true
		cpage.Visible = false
		spage.Visible = false
	end)
	
	sbtn.MouseButton1Click:Connect(function()
		tween(sbtn, "open")
		tween(hbtn, "close")
		tween(ebtn, "close")
		tween(cbtn, "close")
		
		hpage.Visible = false
		epage.Visible = false
		cpage.Visible = false
		spage.Visible = true
	end)
	
	cbtn.MouseButton1Click:Connect(function()
		tween(cbtn, "open")
		tween(hbtn, "close")
		tween(ebtn, "close")
		tween(sbtn, "close")
		
		hpage.Visible = false
		epage.Visible = false
		cpage.Visible = true
		spage.Visible = false
	end)
end;
task.spawn(C_85);
-- StarterGui.Encton.Main.Close.LocalScript
local function C_87()
local script = G2L["87"];
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Main.Visible = false
		script.Parent.Parent.Parent.OpenEncton.Visible = true
	end)
end;
task.spawn(C_87);
-- StarterGui.Encton.Main.ConsoleFrame.scrollconsole.LocalScript
local function C_8b()
local script = G2L["8b"];
	local listlay = script.Parent.UIListLayout -- Reference to the UIListLayout
	
	-- Function to update the CanvasSize dynamically
	local function updateCanvasSize()
		local contentSize = listlay.AbsoluteContentSize -- Get the total size of the content
		script.Parent.CanvasSize = UDim2.new(0, contentSize.X, 0, contentSize.Y) -- Adjust CanvasSize for both width and height
	end
	
	local function createLogMessage(message, messageType)
		local tmplate = script.Parent.TemplateConsole -- Reference to the template
		local clonedoutput = tmplate.consoleoutputmsg:Clone() -- Clone the message template
	
		-- Set text color based on message type
		if messageType == Enum.MessageType.MessageOutput then
			clonedoutput.TextColor3 = Color3.fromRGB(255, 255, 255)
		elseif messageType == Enum.MessageType.MessageWarning then
			clonedoutput.TextColor3 = Color3.fromRGB(255, 200, 0)
		elseif messageType == Enum.MessageType.MessageError then
			clonedoutput.TextColor3 = Color3.fromRGB(255, 50, 50)
		end
	
		-- Set the message text and make it visible
		clonedoutput.Visible = true
		clonedoutput.Text = message
		clonedoutput.Parent = script.Parent -- Add it to the ScrollingFrame
	
		-- Update CanvasSize to reflect new content
		updateCanvasSize()
	end
	
	-- Connect to LogService to listen for console output
	local LogService = game:GetService("LogService")
	LogService.MessageOut:Connect(function(message, messageType)
		createLogMessage(message, messageType)
	end)
	
	-- Adjust CanvasSize when the layout's content size changes
	listlay:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		updateCanvasSize()
	end)
	
end;
task.spawn(C_8b);
-- StarterGui.Encton.Main.Notice.agreebtn.LocalScript
local function C_98()
local script = G2L["98"];
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Notice.Visible = false
		script.Parent.Parent.Parent.background.Visible = false
	end)
end;
task.spawn(C_98);
-- StarterGui.Encton.Main.SettingsFrame.UnlockFPS.EnableDisable.LocalScript
local function C_a0()
local script = G2L["a0"];
	-- close id : 6031068429
	-- open id : 6031068430
	--open color : 0, 0.807843, 0
	-- close color : 188, 0, 0
	
	local toggle = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if toggle == false then
			toggle = true
			script.Parent.Image = "rbxassetid://6031068430"
			script.Parent.ImageColor3 = Color3.new(0, 0.807843, 0)
			setfpscap(1000)
		else
			toggle = false
			script.Parent.Image = "rbxassetid://6031068429"
			script.Parent.ImageColor3 = Color3.new(188, 0, 0)
			setfpscap(120)
		end
	end)
end;
task.spawn(C_a0);
-- StarterGui.Encton.OpenEncton.LocalScript
local function C_ac()
local script = G2L["ac"];
	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.Out, 0, false, 0), {Size = UDim2.new(0, 43,0, 43)}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.2, Enum.EasingStyle.Sine, Enum.EasingDirection.In, 0, false, 0), {Size = UDim2.new(0, 35,0, 29)}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.Main.Visible = true
	end)
end;
task.spawn(C_ac);

return G2L["1"], require;
