		game:GetService("TweenService"):Create(script.Parent.Parent["Server-Side"], TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.In, 0, false, 0), {BackgroundColor3 = Color3.new(0.192, 0.192, 0.192)}):Play()
	end)
end;
