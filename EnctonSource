-- StarterGui.Encton.Main.ExecutorFrame.SaveFrame.SaveBtn.LocalScript
local function C_7e()
local script = G2L["7e"];
	--[[
	
	local btn = script.Parent
	
	local function AddScript(name, source)
		makefolder("Encton")
		writefile("Encton/" .. name, source)
	
		local parent = script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll
		local hub = script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll.Template.NameSaved
		local newhub = hub:Clone()
	
		local scriptname = newhub.scriptName
		local execbutton = newhub.Execute
		local delbutton = newhub.Delete
	
		newhub.Parent = parent
		newhub.Visible = true
		scriptname.Text = name
		execbutton.MouseButton1Click:Connect(function()
			loadstring(source)()
		end)
		delbutton.MouseButton1Click:Connect(function()
			newhub:Destroy()
			delfile("Encton/"..name)
		end)
	end
	for i,v in pairs(script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll:GetChildren()) do
		if v:IsA("Frame") then
			v:Destroy()
		end
	end
	wait(1)
	
	for _, file in ipairs(listfiles("Encton")) do
		AddScript(file:sub(22, #file), readfile(file), true) -- old value: 22
	end
	
	--WORKING METHOD
	for _, files in ipairs(listfiles("Encton")) do
		AddScript(files, readfile(files), true)
	end
	--END
	
	btn.MouseButton1Click:Connect(function()
		AddScript(script.Parent.Parent.InputName.Text, script.Parent.Parent.Parent.Parent.ExecutorFrame.TextHolder.Input.Text, false)
		script.Parent.Parent.InputName.Text = ""
		script.Parent.Parent.Parent.SaveFrame.Visible = false
	end)
	
	
	old source
	--]]
	
	local folderName = "Enctonsc"
	local folderPath = folderName .. "/"
	local btn = script.Parent
	
	if not isfolder(folderName) then
		makefolder(folderName)
	end
	
	local function AddScript(name, source)
		writefile(folderPath .. name, source)
	
	
		local parent = script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll
		local hub = script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll.Template.NameSaved
		local newhub = hub:Clone()
	
		
		local scriptName = newhub.scriptName
		local execButton = newhub.Execute
		local delButton = newhub.Delete
		local loadtoeditorButton = newhub.LoadToEditor
	
		newhub.Parent = parent
		newhub.Visible = true
		scriptName.Text = name
	
	
		execButton.MouseButton1Click:Connect(function()
			loadstring(readfile(folderPath .. name))()
		end)
	
	
		delButton.MouseButton1Click:Connect(function()
			newhub:Destroy()
			delfile(folderPath .. name)
		end)
		
		loadtoeditorButton.MouseButton1Click:Connect(function()
			script.Parent.Parent.Parent.TextHolder.Input.Text = readfile(folderPath .. name)
		end)
	end
	
	
	for _, v in pairs(script.Parent.Parent.Parent.Parent.HomeFrame.SavedScripts.scroll:GetChildren()) do
		if v:IsA("Frame") then
			v:Destroy()
		end
	end
	wait(1)
	
	
	for _, file in ipairs(listfiles(folderPath)) do
		local fileName = file:match("([^/]+)$")
		AddScript(fileName, readfile(file))
	end
	
	
	btn.MouseButton1Click:Connect(function()
		local scriptName = script.Parent.Parent.InputName.Text
		local scriptSource = script.Parent.Parent.Parent.Parent.ExecutorFrame.TextHolder.Input.Text
	
		if scriptName and scriptName ~= "" and scriptSource and scriptSource ~= "" then
			AddScript(scriptName, scriptSource)
			script.Parent.Parent.InputName.Text = ""
			script.Parent.Parent.Parent.SaveFrame.Visible = false
		end
	end)
	
end;
task.spawn(C_7e);
