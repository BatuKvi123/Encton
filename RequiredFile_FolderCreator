-- just some couple of things here, ignore this part :D

--Encton Sc Folder
if not isfolder("Enctonsc") then
makefolder("Enctonsc")
end

--Encton Workspace Folder
if not isfolder("EnctonWorkspace") then
makefolder("EnctonWorkspace")
end

--Encton Settings Folder
if not isfolder("EnctonSettings") then
makefolder("EnctonSettings")
end

--EnctonEditorText File
if not isfile("EnctonWorkspace/EnctonEditorText.lua") then
writefile("EnctonWorkspace/EnctonEditorText.lua", "")
end

--Encton KeyInput File
if not isfile("EnctonWorkspace/EnctonKeyInput.lua") then
	writefile("EnctonWorkspace/EnctonKeyInput.lua", "")
end

--Encton KeyInput Premium File
if not isfile("EnctonWorkspace/EnctonKeyInputPremium.lua") then
	writefile("EnctonWorkspace/EnctonKeyInputPremium.lua", "")
end

--Encton Auto Check Key File
if not isfile("EnctonSettings/AutoCheckKey.lua") then
	writefile("EnctonSettings/AutoCheckKey.lua", "Disabled")
end

if not isfile("EnctonWorkspace/popup.lua") then
writefile("EnctonWorkspace/popup.lua", "Disabled")
end
