-- just some couple of things here, ignore this part :D

--Encton Sc Folder
if not isfolder("Enctonsc") then
makefolder("Enctonsc")
end

--Encton Workspace Folder
if not isfolder("EnctonWorkspace") then
makefolder("EnctonWorkspace")
end

--EnctonEditorText File
if not isfile("EnctonWorkspace/EnctonEditorText") then
writefile("EnctonWorkspace/EnctonEditorText", "")
end

--Encton KeyInput File
if not isfile("EnctonWorkspace/EnctonKeyInput") then
	writefile("EnctonWorkspace/EnctonKeyInput", "")
end
