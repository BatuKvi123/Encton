--Encton Sc Folder
if not isfolder("Enctonsc") then
makefolder("Enctonsc")
end

--Encton Workspace Folder
if not isfolder("EnctonWorkspace") then
makefolder("EnctonWorkspace")
end

--EnctonEditorText File
if not isfile("EnctonWorkspace/EnctonEditorText.lua") then
writefile("EnctonWorkspace/EnctonEditorText.lua", "")
end

--Encton KeyInput File
